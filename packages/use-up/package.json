    "@cherrypulp/i18n": "^1.4.6",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.16.13",
    "axios": "^0.23.0",
    "axios-es6": "^0.11.1",
    "graphql": "^15.6.1",
    "js-config-helper": "^1.0.5",
    "js-form-helper": "^1.0.9"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.7",
    "lerna": "^4.0.0",
    "postcss": "^8.3.9",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.8.3"
  }
