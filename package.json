{
  "name": "up-react",
  "description": "React toolkit of multiple plugins to prototype your app quickly",
  "version": "18.1.8",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "main": "dist/index.umd.js",
  "exports": {
    ".": {
      "module": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.umd.js"
    }
  },
  "module": "dist/index.es.js",
  "browser": "dist/index.cjs.js",
  "unpkg": "dist/index.cjs.js",
  "jsdelivr": "dist/index.cjs.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "publish": "bash publish.sh",
    "types": "npx tsc --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.6.3",
    "core-js": "^3.31.0",
    "cross-fetch": "^3.1.6",
    "graphql": "~16.6.0",
    "i18next": "^23.2.3",
    "js-config-helper": "^1.3.0",
    "js-form-helper": "^1.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "react-redux": "^8.1.1",
    "react-use": "^17.4.0",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/ui": "^0.32.2",
    "happy-dom": "^9.20.3",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.5",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
