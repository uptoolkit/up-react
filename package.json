{
  "name": "up-react",
  "description": "React toolkit of multiple plugins to prototype your app quickly",
  "version": "2.0.2",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "main": "dist/index.umd.js",
  "exports": {
    ".": {
      "module": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.umd.js"
    },
    "./": "./"
  },
  "module": "dist/index.es.js",
  "browser": "dist/index.cjs.js",
  "unpkg": "dist/index.cjs.js",
  "jsdelivr": "dist/index.cjs.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "types": "npx tsc --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@reduxjs/toolkit": "^1.8.6",
    "antd": "^4.23.5",
    "core-js": "^3.16.4",
    "cross-fetch": "^3.1.5",
    "graphql": "^15.8.0",
    "i18next": "^21.10.0",
    "js-config-helper": "^1.3.0",
    "js-form-helper": "^1.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-redux": "^8.0.4",
    "react-use": "^17.3.1",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-test-renderer": "^17.0.2",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "latest",
    "happy-dom": "^7.5.12",
    "jest": "^25.5.4",
    "jsdom": "latest",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.8.4",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
